<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.Andrew.statusbar.Andrew1Activity" >

    <Button
        android:id="@+id/l1_button1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="48dp"
        android:text="BasicNotifications" />

    <Button
        android:id="@+id/l1_button2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/l1_button1"
        android:layout_below="@+id/l1_button1"
        android:text="CustomNotifications" />

    <Button
        android:id="@+id/l1_button3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/l1_button2"
        android:layout_below="@+id/l1_button2"
        android:text="LNotifications" />

    <Button
        android:id="@+id/l1_button4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/l1_button3"
        android:layout_centerVertical="true"
        android:text="MessagingService" />

    <Button
        android:id="@+id/l1_button5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/l1_button4"
        android:layout_below="@+id/l1_button4"
        android:text="eric" />

    <TextView
        android:id="@+id/ldtextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/l1_button5"
        android:layout_below="@+id/l1_button5"
        android:layout_marginLeft="67dp"
        android:layout_marginTop="40dp"
        android:text="TextView" />

</RelativeLayout>
